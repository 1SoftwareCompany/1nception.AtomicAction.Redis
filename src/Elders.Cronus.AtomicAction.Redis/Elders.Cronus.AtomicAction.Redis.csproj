<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net472</TargetFrameworks>
  </PropertyGroup>

  <!--Nuget-->
  <PropertyGroup>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <PackageId>Cronus.AtomicAction.Redis</PackageId>
    <PackageLicenseUrl>https://github.com/Elders/Cronus.AtomicAction.Redis/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/Elders/Cronus.AtomicAction.Redis</PackageProjectUrl>
    <PackageTags>distrubited lock redis redlock cronus</PackageTags>
    <RepositoryUrl>https://github.com/Elders/Cronus.AtomicAction.Redis</RepositoryUrl>
    <RepositoryType>Framework</RepositoryType>
    <Authors>Elders</Authors>
    <PackageIconUrl>https://raw.githubusercontent.com/Elders/logo/master/cronus/frame.png</PackageIconUrl>
  </PropertyGroup>

  <!--netstandard2.0-->
  <PropertyGroup>
    <DefineConstants>TRACE;DEBUG;</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.CSharp" Version="4.5.0" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="LibLog" Version="5.0.2" />
    <PackageReference Include="Cronus" Version="5.0.0-discovery0014" />
    <PackageReference Include="RedLock" Version="4.0.0" />
  </ItemGroup>

  <PropertyGroup>
    <DebugType>portable</DebugType>
    <DebugSymbols>true</DebugSymbols>
    <EmbedSources>true</EmbedSources>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="SourceLink.Create.CommandLine" Version="2.8.3" PrivateAssets="All" />
  </ItemGroup>
</Project>
